{
    "version": 2,
    "builds": [
      {
        "src": "html/index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/html/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/html/index.html"
      }
    ]
  }